$("#change-<%=@side%>-button-prev").attr('href', "/albums/<%=@album.id%>/change_cover?index=<%=@index - 1%>&side=<%=@side%>&direction=prev");
$("#change-<%=@side%>-button-next").attr('href', "/albums/<%=@album.id%>/change_cover?index=<%=@index + 1%>&side=<%=@side%>&direction=next");
$("#<%=@side%>-cover-image").attr('src', "<%=@src%>");
$(".cover-loader").hide();
<% if @errors %>
  $("#change-<%=@side%>-button").addClass("disabled")
<% end %>